
.subgraphInput #title {
    background-color: #bb0000;
}

.subgraphInput #icon {
    background-image: resource("arrow-right-solid");
    -unity-background-scale-mode: scale-to-fit;
    min-width: 16px;
    margin: 2px;
}

.subgraphInput #title .unity-base-field {
    width: 100%;
    flex-shrink: 1;
}

.subgraphInput #title #unity-text-input {
    background-color: rgba(0, 0, 0, 0);
    border-width: 0;
}
